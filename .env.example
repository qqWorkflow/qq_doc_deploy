# Git repository URL (SSH format)
GIT_REPO=git@github.com:qqWorkflow/qq_doc.git

# Path to SSH private key (relative to docker-compose.yml)
SSH_KEY_PATH=./.ssh/deploy_key

# ===========================================
# Optional settings (defaults shown)
# ===========================================

# Git branch to sync
# GIT_BRANCH=main

# Sync interval (how often to pull from git)
# SYNC_PERIOD=60s

# HTTP port for nginx
# HTTP_PORT=80
